{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Luiscruzgmu Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
      "runtimeArgs": [
        "--inspect"
      ],
      "args": [
        "dev",
        "-p",
        "3333"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Luiscruzgmu Browser",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "urlFilter": "http://localhost:3333/*",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "Luiscruzgmu (Full Stack)",
      "configurations": ["Luiscruzgmu Server", "Luiscruzgmu Browser"],
      "presentation": {
        "group": "Luiscruzgmu"
      }
    }
  ]
}